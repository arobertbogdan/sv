<p id="notice"><%= notice %></p>

<h1>Listing Posts</h1>

<table>

  <tbody>
  <% @posts.each do |post| %>
      <tr>
        <td>
          <a class="up_vote" post-id=<%= post.id %>>+</a>
          <%= post.rating %>
          <a class="down_vote" post-id=<%= post.id %>>-</a>
        </td>
        <td><%= link_to post.title, post %></td>
        <td><%= post.description %></td>
        <td><%= post.media %></td>
      </tr>
  <% end %>
  </tbody>
</table>