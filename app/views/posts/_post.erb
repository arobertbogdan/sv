<tr>
  <td>
    <a class="up_vote" post-id=<%= post.id %>>+</a>
    <%= post.rating %>
    <a class="down_vote" post-id=<%= post.id %>>-</a>
  </td>
  <td><%= link_to post.title, post %></td>
  <td><%= post.description %></td>
  <td><%= post.media %></td>
  <td>submited <span><%= time_ago_in_words post.created_at %></span> ago</td>
  <td><%= link_to post.count_comments, post %></td>
</tr>